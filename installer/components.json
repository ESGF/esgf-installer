    "extract_dir": "/tmp/thredds"
  },
  "java": {
    "version": "1.8.0_162",
    "url": "http://aims1.llnl.gov/esgf/dist/2.6/8/java/{version}/jdk{version}-64.tar.gz",
    "extract_dir": "/tmp/java",
    "tar_root_dir": "jdk{version}"
  },
  "mod_wsgi": "4.5.3",
  "esgcet": {
    "tag": "v3.5.0",
